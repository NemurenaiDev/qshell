{
    "name": "qshell",
    "license": "MIT",
    "version": "0.2.0",
    "module": "app.ts",
    "bin": "./build/src/app.js",
    "scripts": {
        "nix-update": "rm -rf ./node_modules; node2nix -18 --node-env ./nix/node-env.nix --composition ./nix/default.nix --output ./nix/node-package.nix",
        "build": "npx tsc && npx pkg . --targets node18-linux-x64 --output ./build/qshell",
        "postinstall": "npm rebuild node-pty --target=18.20.8 --runtime=node"
    },
    "dependencies": {
        "@types/node": "^22.14.0",
        "@types/yargs": "^17.0.33",
        "@yao-pkg/pkg": "^6.3.2",
        "node-pty": "^1.0.0",
        "typescript": "^5.0.0",
        "yargs": "^17.7.2"
    }
}
